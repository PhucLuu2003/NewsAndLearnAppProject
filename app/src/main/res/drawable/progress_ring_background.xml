    <!-- Background ring -->
    <item>
        <shape android:shape="ring"
            android:innerRadiusRatio="2.5"
            android:thickness="4dp"
            android:useLevel="false">
            <solid android:color="#33FFFFFF" />
        </shape>
    </item>
</layer-list>
